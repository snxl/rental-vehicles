{
  "name": "happmoby",
  "version": "1.0.0",
  "description": "Documentation integration",
  "author": "@snxl",
  "main": "dist/src/application/main/server.js",
  "license": "ISC",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "node -r dotenv/config dist/src/application/main/server.js",
    "dev": "NODE_ENV=development ts-node-dev -r dotenv/config --inspect --transpile-only --respawn --ignore-watch node_modules src/application/main/server.ts",
    "clean": "rimraf dist/",
    "build": "npm run clean && tsc",
    "lint": "tsc && eslint . && npm run clean",
    "lint:fix": "eslint --fix",
    "test": "npm run test:unit && npm run test:integration",
    "test:watch": "jest -c ./test/unit/jest.config.js --watchAll --passWithNoTests",
    "test:unit": "jest -c ./test/unit/jest.config.js --passWithNoTests",
    "test:integration": "jest -c ./test/integration/jest.config.js --runInBand --passWithNoTests"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "express-http-context": "^1.2.4",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.5",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "objection": "^3.0.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "rimraf": "^3.0.2",
    "swagger-ui-express": "^4.3.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.181",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.23",
    "@types/response-time": "^2.3.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}
